<h1>Students list</h1>

<%= form_tag students_path, :method => 'get' do %>
  <%= label_tag(:query, "Search")%>

  <%= text_field_tag :query, params[:query]%>
  <%= submit_tag("Search", :name=>nil)%>
<%end%>

<ol>
  <%@students.each do |s|%>
  <li><%=render partial: 'student', locals:{student: s}%></li>
  <%end%>
</ol>
